{
  "name": "@ianwalter/base-error",
  "version": "4.0.1",
  "description": "A general base Error class that can be extended to create custom Error classes",
  "main": "dist/base-error.js",
  "module": "dist/base-error.m.js",
  "iife": "dist/base-error.iife.js",
  "scripts": {
    "lint": "eslint index.js test.js",
    "dist": "dist --babel --no-iife",
    "iife": "dist --name BaseError --babel --inline --no-esm --no-cjs",
    "pretest": "npm run dist && npm run iife",
    "test": "ava"
  },
  "keywords": [
    "base",
    "error",
    "custom",
    "stacktrace"
  ],
  "author": "Ian Walter <public@iankwalter.com> (https://iankwalter.com)",
  "license": "SEE LICENSE IN LICENSE",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/runtime": "^7.2.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@ianwalter/dist": "^1.5.1",
    "ava": "^1.0.1",
    "eslint": "^5.7.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "np": "^4.0.1"
  },
  "browserslist": [
    "> 0.5%"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "eslint:recommended",
      "standard"
    ]
  }
}
